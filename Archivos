#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <stddef.h>

int main()
{
   FILE *ap;
   char *save;
   char *elemento;
   int cp;
   

   ap = fopen("df.txt", "rb");
   if(ap == NULL)
   {
      puts("Error");
   }
   else
   {
      puts("Ingrese el cp buscado");
      scanf("%d", &cp);
      do{
         elemento = strtok_r((char*)ap, "|", &save);
         
      }while(strcmp(elemento, cp) == 0);
   while(elemento != NULL)
   { 
      elemento = strtok_r(NULL, "|", &save);
      printf("%s", elemento);
   }
   }
}

//strcmp()
